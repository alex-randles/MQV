:hasRefinementQuery rdfs:comment "A query executed during a refinement." ;
                    rdfs:label "has refinement query"@en ;
                    <http://www.w3.org/2004/02/skos/core#example> """@prefix :     <http://example.com/>.;
@prefix mqv: <http://ontology.example.org/mqv#>.;

:refinement1 a mqv:Refinement;
	mqv:hasRefinementQuery '''
		PREFIX rr: <http://www.w3.org/ns/r2rml#>
		PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
		PREFIX foaf: <http://xmlns.com/foaf/0.1/>
		DELETE{?om rr:datatype ?datatype}
		INSERT{?om rr:datatype xsd:string }
		WHERE
		{
		?sub rr:predicateObjectMap ?pom.
		?pom rr:predicate foaf:name.
		?pom rr:objectMap ?om.
		?om rr:datatype ?datatype.
		}'''^^xsd:string .
""" .


:hasValue rdfs:comment "The value associated with the violation." ;
          rdfs:label "has value"@en ;
          <http://www.w3.org/2004/02/skos/core#example> """@prefix :     <http://example.com/>.;
@prefix mqv: <http://ontology.example.org/mqv#>.;

:violation-0
	a mqv:MappingViolation;
	mqv:hasValue \"irish\".
""" .


:isDescribedBy rdfs:comment "The location of the violation within the mapping." ,
                            "The metric which detected the violation." ,
